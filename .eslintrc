{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"strict": 0,
		"no-console": 0,
		"no-tabs": 0,
		"no-loop-func": 1,
		"no-unused-vars": 1,
		"indent": [
			0,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"no-debugger": 0,
		"class-methods-use-this": 0,
		"quotes": [
			2,
			"double",
			{
				"avoidEscape": true
			}
		],
		"comma-dangle": ["error", "never"],
		"max-len": 0,
		"arrow-body-style": 0,
		"linebreak-style": 0,
		"no-param-reassign": [
			2,
			{
				"props": false
			}
		],
		"arrow-parens": [0, "as-needed"],
		"function-paren-newline": [2, "consistent"],
		"object-curly-newline": [
			0,
			{
				"ObjectExpression": "always",
				"ObjectPattern": {
					"multiline": true
				},
				"ImportDeclaration": "never",
				"ExportDeclaration": {
					"multiline": true,
					"minProperties": 3
				}
			}
		],
		"no-case-declarations": 0,
		"lines-between-class-members": ["error", "always"],
		"no-plusplus": 0,
		"no-mixed-operators": 0,
		"prettier/prettier": ["error"]
	}
}
